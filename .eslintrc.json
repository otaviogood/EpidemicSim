{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "parser": "esprima"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-inferrable-types": "off",
        "prefer-const": "off"
    }
}
