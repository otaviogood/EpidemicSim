namespace Flatbuf;
table Person {
    homeIndex:int32;
    officeIndex:int32;
    supermarketIndex:int32;
    hospitalIndex:int16;
    countyIndex:int16;
    name:string;
    age:uint8;
    maleFemale:uint8;  // 0 is male, 1 is female
    race:uint8;

}

table PersonArray {
    people:[Person];
    junk:int32;
}

root_type PersonArray;